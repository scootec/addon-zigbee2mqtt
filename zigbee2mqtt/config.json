{
  "name": "Zigbee2mqtt",
  "version": "1.3.9",
  "slug": "zigbee2mqtt",
  "description": "Zigbee2MQTT add-on by Scootec",
  "url": "https://github.com/scoootec/addon-zigbee2mqtt",
  "startup": "application",
  "ingress": true,
  "panel_icon": "mdi:zigbee",
  "uart": true,
  "arch": ["aarch64", "amd64", "armhf", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant": "2021.7.0",
  "map": ["config:rw"],
  "services": ["mqtt:need"],
  "options": {
    "zigbee_herdsman_debug": false
  },
  "schema": {
    "zigbee_herdsman_debug": "bool?"
  }
}
